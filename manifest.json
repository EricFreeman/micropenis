{
"manifest_version": 2,
    "name": "micropenis",
    "description": "Change every instance of 'microservice' to 'micropenis'",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}